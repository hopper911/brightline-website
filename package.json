{
  "name": "brightline-website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "dotenv -e ./lib/.env -- next dev",
    "build": "set -a; . ./lib/.env; set +a; SCHEMA=$([ \"$VERCEL\" = \"1\" ] && echo prisma/schema.postgres.prisma || echo prisma/schema.prisma); prisma migrate deploy --schema $SCHEMA && next build",
    "build:vercel": "set -a; . ./lib/.env; set +a; prisma generate --schema prisma/schema.postgres.prisma && prisma db push --schema prisma/schema.postgres.prisma && node prisma/seed.mjs && next build",
    "start": "dotenv -e ./lib/.env -- next start",
    "lint": "eslint",
    "postinstall": "set -a; . ./lib/.env; set +a; SCHEMA=$([ \"$VERCEL\" = \"1\" ] && echo prisma/schema.postgres.prisma || echo prisma/schema.prisma); prisma generate --schema $SCHEMA",
    "prisma:generate": "prisma generate --schema prisma/schema.prisma",
    "prisma:generate:pg": "prisma generate --schema prisma/schema.postgres.prisma",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:migrate:pg": "prisma migrate deploy --schema prisma/schema.postgres.prisma",
    "prisma:studio": "prisma studio",
    "db:seed": "node prisma/seed.mjs"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "next": "16.1.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.23",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.2",
    "postcss": "^8.5.6",
    "prisma": "^5.20.0",
    "tailwindcss": "^3.4.19",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
